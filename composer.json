    "keywords": ["vdhoangson", "role", "permission", "multi", "management", "laravel", "pencms"],
    "homepage": "https://github.com/vdhoangson/permission",
    "license": "MIT",
    "authors": [
        {
            "name": "vdhoangson",
            "email": "vdhoangson@gmail.com"
        }
